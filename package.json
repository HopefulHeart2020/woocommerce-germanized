    "grunt-sass": "3.0.2",
    "grunt-shell": "3.0.1",
    "grunt-stylelint": "0.11.1",
    "grunt-wp-i18n": "1.0.3",
    "istanbul": "1.0.0-alpha.2",
    "mocha": "6.2.0",
    "node-sass": ">=4.13.1",
    "prettier": "github:automattic/calypso-prettier#c56b4251",
