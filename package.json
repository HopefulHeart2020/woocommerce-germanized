    "babel-preset-stage-2": "6.24.1",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "config": "3.2.2",
    "cross-env": "5.2.0",
    "eslint": "6.5.1",
    "eslint-config-wpcalypso": "4.0.1",
    "eslint-plugin-wpcalypso": "4.1.0",
    "grunt": "1.0.4",
